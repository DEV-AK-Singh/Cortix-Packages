{
  "name": "cortix",
  "version": "1.0.0",
  "description": "Cortix is an **AI-powered DevOps automation system** that transforms any GitHub repository into **production-ready infrastructure**. With a single GitHub link, Cortix analyzes the entire codebase, identifies the tech stack, visualizes the project structure, and automatically generates YAML configs, Dockerfiles, and Bash setup scripts - all powered by **Gemini 2.5 Flash**.",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "tsc && node dist/src/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --watch --respawn src/server.ts",
    "worker": "ts-node-dev --watch --respawn src/modules/jobs/jobs.worker.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.66.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "prisma": "^6.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pg": "^8.16.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
